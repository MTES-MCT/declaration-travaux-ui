    "@deck.gl/react": "^7.0.11",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@turf/area": "^6.0.1",
    "@turf/centroid": "^6.0.2",
    "@turf/line-intersect": "^6.0.2",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.19.0",
    "env-cmd": "^9.0.3",
    "formik": "^1.5.7",
    "keycloak-js": "^6.0.1",
    "lodash": "^4.17.11",
    "mdx.macro": "^0.2.8",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-async": "^6.2.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-icons": "^3.7.0",
    "react-lazyload": "^2.6.1",
    "react-map-gl": "^5.0.2",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "3.0.1",
    "yup": "^0.27.0"
  },
  "scripts": {
    "prettier": "prettier --single-quote --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint": "eslint src/",
    "start": "react-scripts start",
    "start:dev": "env-cmd npm run start",
    "start:staging": "env-cmd .env.staging npm run start",
    "start:test": "env-cmd .env.test npm run start",
    "start:production": "env-cmd .env.production npm run start",
    "build": "react-scripts build",
    "build:dev": "env-cmd npm run build",
    "build:staging": "env-cmd .env.staging npm run build",
    "build:test": "env-cmd .env.test npm run build",
    "build:production": "env-cmd .env.production npm run build",
    "test:unit": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "predeploy:dev": "npm run build:dev",
    "predeploy:test": "npm run build:test",
    "predeploy:staging": "npm run build:staging",
    "predeploy:production": "npm run build:production",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "gh-pages -d build -m 'Deploy on Github pages [ci skip]'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react-testing-library": "^7.0.1",
    "source-map-explorer": "^1.8.0",
    "start-server-and-test": "^1.9.1"
  }
}
