    "email": "tristan.robert.44@gmail.com",
    "name": "Tristan Robert",
    "url": "https://github.com/tristanrobert"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0-beta.2",
    "@material-ui/icons": "^4.0.0-beta.0",
    "final-form": "^4.12.0",
    "markdown-to-jsx": "^6.9.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^4.1.0",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prettier": "prettier --single-quote --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint": "eslint src/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jest-dom": "^3.2.2",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react-testing-library": "^7.0.0",
    "source-map-explorer": "^1.8.0"
  }
}
